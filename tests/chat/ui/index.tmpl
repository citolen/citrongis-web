<% include('ui/style.css') %>
<% include('ui/bootstrap.min.css') %>

<div id="chat_root">
    <h1 id="chat_title">Chat</h1>
    <div id="chat_content">
        <br/>
        <input type="text" id="chat_text_entry" onkeypress="<%= trigger(chat_keypress) %>"/>
        <button id="chat_send_btn" onclick="<%= trigger(chat_click) %>">send</button>
        <br />
        <textarea id="chat_area"></textarea>
    </div>
</div>